{
  "suggest.noselect": true,
  "eslint.autoFixOnSave": false,
  "eslint.quiet": false,
  "eslint.options": {
    "overrideConfig": {
      "rules": {
        "@typescript-eslint/strict-boolean-expressions": "off"
      }
    }
  },
  "coc.preferences.formatOnSaveFiletypes": ["css", "markdown", "typescriptreact", "rust"],
  "css.validate": false,
  "python.linting.mypyEnabled": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.updateImportsOnFileMove.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": false,
  "tsserver.useLocalTsdk": true,
  "rust-client.enable": false,
  "python.pythonPath": "/usr/bin/python3",
  "python.venvPath": "/home/discord/.virtualenvs",
  "python.formatting.provider": "ruff",
  "python.linting.ruffEnabled": true,
  "pyright.organizeimports.provider": "ruff"
}
