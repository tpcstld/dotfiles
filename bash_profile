# graphical vim
# alias vim='gvim'
# ls aliases
alias ls='ls -l --color'
alias dir='ls -l --color'

# Git aliases

alias gs='git status'
alias gaa='git add --all :/'
alias gcaa='git commit --amend -a'
alias gca='git commit --amend'
alias gd='git diff'

# A vim command for the terminal
alias :q='exit'

# Autojump
. /usr/share/autojump/autojump.sh

alias open='xdg-open'
